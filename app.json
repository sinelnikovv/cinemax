{
  "expo": {
    "name": "Cinemax",
    "slug": "Cinemax",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#171725",
    "splash": {
      "image": "./src/assets/images/splashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#171725"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sinelnikov.cinemax"
    },
    "androidStatusBar": {
      "translucent": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo.png",
        "backgroundColor": "#171725"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
